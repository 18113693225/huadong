<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomEditTextAttrs">
        <attr name="chint" format="string" />
        <attr name="allow_null" format="boolean" />
        <attr name="edit_inputtype1" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomEditText2Attrs">
        <attr name="chint2" format="string" />
        <attr name="t_text2" format="string" />
        <attr name="show_image" format="boolean" />
        <attr name="show_image_date" format="boolean" />
        <attr name="show_Button" format="boolean" />
        <attr name="show_unit" format="boolean" />
        <attr name="unit_text" format="string" />
        <attr name="button_text" format="string" />
        <attr name="edit_singleline" format="boolean" />
        <attr name="edit_maxline" format="integer" />
        <attr name="edit_minline" format="integer" />
        <attr name="edit_inputtype" format="string" />
        <attr name="readonly" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomEditText3Attrs">
        <attr name="chint3" format="string" />
