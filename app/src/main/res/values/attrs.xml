    </declare-styleable>
    <declare-styleable name="CustomAttrsButton">
        <attr name="selected" format="boolean" />
        <attr name="button_value" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomAttrsImageButton">
        <attr name="text" format="string" />
        <attr name="textSize" format="integer" />
        <attr name="src" format="reference"></attr>
        <attr name="orientation" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="AutoText">
        <attr name="mytextSize" format="dimension" />
        <attr name="mytextColor" format="reference|color" />
        <attr name="lineSpacingExtra" format="dimension" />
        <attr name="typeface" format="dimension" />
    </declare-styleable>

</resources>