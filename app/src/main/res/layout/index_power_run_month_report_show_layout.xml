<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:CustomAttrs="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical" >

    <include layout="@layout/common_title" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:orientation="vertical" >

            <include layout="@layout/common_gps_title" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_pname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/tt_new_pname" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_runit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/tt_new_runit" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_rperson"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/tt_new_rperson" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_aqyxts"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_aqyxts" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_bydzcz"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_bydzcz" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_byqzdl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_byqzdl" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_zysbzgfh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_zysbzgfh" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_qzxs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_qzxs" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_sbdqsyqh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_sbdqsyqh" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_byqfjt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_byqfjt" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_qzdzhd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_qzdzhd" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_byzysb"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_byzysb" />

            <com.demo.app.view.CustomeTextView
                android:id="@+id/pr_month_show_fxcl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                CustomAttrs:name_text="@string/pr_month_fxcl" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:orientation="horizontal" >

                <Button
                    android:id="@+id/edit_pr_tables"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="20dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_franchise_shape"
                    android:drawableLeft="@drawable/icon_reedit"
                    android:text="重新编辑"
                    android:textStyle="bold" />

                <Button
                    android:id="@+id/delete_pr_tables"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/button_franchise_shape1"
                    android:drawableLeft="@drawable/icon_del"
                    android:text="删除表单"
                    android:textStyle="bold" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>